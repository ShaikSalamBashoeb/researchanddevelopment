<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="point" class="com.spring.Point"  scope="prototype">
    	<property name="pointX" value="1"></property>
    	<property name="pointY" value="2"></property>
    </bean>
    
    <bean id="triangle"  class="com.spring.Triangle" autowire="byName" scope="prototype">
    </bean>
</beans>
